{
  "defaultSeverity": "error",
  "extends": [
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "import-name": false,
    "variable-name": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  },
  "rulesDirectory": []
}